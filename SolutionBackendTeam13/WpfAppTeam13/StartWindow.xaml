<Window x:Class="WpfAppTeam13.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppTeam13"
        mc:Ignorable="d"
        Title="StartWindow" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="True">
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="File">
                    <MenuItem Header="Student" Name="menuNewStudent" Click="StudentMenu_Click"></MenuItem>
                    <MenuItem Header="Sql window" Name="SqlOpen" Click="SqlMenu_Click"></MenuItem>
                    <MenuItem Header="Close" Name="menuClose" Click="menuClose_Click"></MenuItem>
                </MenuItem>

            </Menu>
            <StatusBar DockPanel.Dock="Bottom">

            </StatusBar>
            <ListBox>

            </ListBox>
        </DockPanel>
    </Grid>
</Window>
